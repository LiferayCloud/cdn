{
  "id": "hosting",
  "image": "wedeploy/hosting:2.0.1",
  "loadBalancer": {
    "cdn": true,
    "targetPort": 80,
    "customDomains": ["cdn.liferay.cloud"]
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  }
}